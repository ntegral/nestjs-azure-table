{
  "name": "@nestjs/azure-table",
  "version": "1.0.0",
  "description": "The Azure Table Storage module for Nest framework (node.js)",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ntegral/nestjs-azure-table"
  },
  "contributors": [
    {
      "name": "Dexter Hardy",
      "email": "dexter.hardy@ntegral.com",
      "url": "https://www.ntegral.com"
    }
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "format": "prettier --write \"lib/**/*.ts\"",
    "publish:npm": "npm publish --access public",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "keywords": [
    "nestjs",
    "azure storage table",
    "azure",
    "node.js"
  ],
  "author": "Dexter Hardy",
  "license": "ISC",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "lib",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.2",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "dotenv": "^16.0.1",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^28.1.2",
    "npm-install-peers": "^1.2.2",
    "prettier": "^2.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.5",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@azure/data-tables": "^13.1.2",
    "@nestjs/common": "^9.0.3",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.3"
  }
}
